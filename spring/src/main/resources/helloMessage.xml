<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorld" class="cn.fly.learn.spring.chapter01.HelloWorld"></bean>
    <bean id="helloChina" class="cn.fly.learn.spring.chapter01.HelloChina"></bean>
    <bean id="person" class="cn.fly.learn.spring.chapter01.Person">
        <property name="helloMessage" ref="helloChina"></property>
    </bean>

</beans>